<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.String?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Text?>

<AnchorPane minHeight="100" prefWidth="750.0" styleClass="dialogContainer" xmlns="http://javafx.com/javafx/8.0.999-ea" xmlns:fx="http://javafx.com/fxml/1">
  <children>
    <BorderPane prefWidth="200.0" styleClass="dialogContent" AnchorPane.bottomAnchor="30.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
      <top>
        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Scegli gli effetti da utilizzare">
          <styleClass>
            <String fx:value="text" />
            <String fx:value="title" />
          </styleClass>
        </Text>
      </top>
      <center>
        <VBox alignment="CENTER" styleClass="dialogContent" BorderPane.alignment="CENTER" spacing="25">
          <VBox fx:id="effectsVBox" alignment="CENTER_LEFT" prefWidth="200.0" spacing="5.0">
          </VBox>
          <VBox fx:id="subEffectsVBox" alignment="CENTER_LEFT" prefWidth="200.0" spacing="5.0">
          </VBox>
          <VBox fx:id="anyTimeVBox" alignment="CENTER_LEFT" prefWidth="200.0" spacing="0.0">
          </VBox>
        </VBox>
      </center>
      <bottom>
        <Button fx:id="buttonNext" alignment="CENTER" defaultButton="true" mnemonicParsing="false" text="Seleziona" BorderPane.alignment="BOTTOM_CENTER" />
      </bottom>
    </BorderPane>
  </children>

</AnchorPane>
